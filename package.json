{
  "name": "elm-live",
  "version": "1.0.4",
  "description": "Elm development with LiveReload. Feels lightning-fast!",
  "bin": {
    "elm-live": "bin/elm-live.js"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  },
  "peerDependencies": {
    "elm": "*"
  },
  "scripts": {
    "develop": "node-dev --respawn --no-notify --no-deps test.js",
    "manpages": "scripts/manpages.sh",
    "test": "eslint . && node test.js"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "files": [
    "**/*.js",
    "/Readme.md",
    "/License.md"
  ],
  "directories": {
    "man": "/manpages"
  },
  "license": "MIT",
  "keywords": [
    "cli",
    "tool"
  ],
  "author": "Tomek Wiszniewski <t.wiszniewski@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:tomekwi/elm-live"
  },
  "devDependencies": {
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "node-dev": "github:tomekwi/node-dev#39f8c44",
    "prova": "^2.1.2",
    "q-stream": "^0.2.0"
  }
}
